pipeline{
    
    agent any

    stages{
        stage("echo"){
            input {
                 message 'Do you want to continue?'
                ok 'venkat'
            }
            steps{
                echo 'This is echo build stage'
            }
           
        }
        stage("Shell command"){
            steps{
                sh '''date
                cal 2023
                pwd'''
            }
           
        }
    }
    
}
